@article{ParkRavani95,
   author = {Park, F. C. and Ravani, B.},
   title = {Bezier curves on Riemannian-manifolds and lie-groups with kinematics applications},
   journal = {ASME Journal of Mechanical Design},
   volume = {117},
   number = {1},
   pages = {36-40},
   note = {Article},
   abstract = {In this article we generalize the concept of Bezier curves to curved spaces, and illustrate this generalization with an application in kinematics. We show how De Casteljau's algorithm for constructing Bezier curves can be extended in a natural way to Riemannian manifolds. We then consider a special class of Riemannian manifold, the Lie groups. Because of their group structure Lie groups admit an elegant, efficient recursive algorithm for constructing Bezier curves. Spatial displacements of a rigid body also form a Lie group, and can therefore be interpolated (in the Bezier sense) using this recursive algorithm. We apply this algorithm to the kinematic problem of trajectory generation or motion interpolation for a moving rigid body. The orientation trajectory of motions generated in this way have the important property of being invariant with respect to choices of inertial and bodyfixed reference frames.},
   year = {1995}
}

@article{ParkRavani97,
   author = {Park, F. C. and Ravani, B.},
   title = {Smooth invariant interpolation of rotations},
   journal = {ACM Transactions on Graphics},
   volume = {16},
   number = {3},
   pages = {277-295},
   note = {Article},
   abstract = {We present an algorithm for generating a twice-differentiable curve on the rotation group SO(3) that interpolates a given ordered set of rotation matrices at their specified knot times. In our approach we regard SO(3) as a Lie group with a bi-invariant Riemannian metric, and apply the coordinate-invariant methods of Riemannian geometry. The resulting rotation curve is easy to compute, invariant with respect to fixed and moving reference frames, and also approximately minimizes angular acceleration.},
   year = {1997}
}

@article{
   author = {Park, F. C. and Yu, J. and Chun, C. and Ravani, B.},
   title = {Design of developable surfaces using optimal control},
   journal = {ASME Journal of Mechanical Design},
   volume = {124},
   number = {4},
   pages = {602-608},
   note = {Article},
   abstract = {This paper formulates the developable surface design problem in an optimal control setting Given a regular curve b(t) an the unit sphere corresponding to a one-parameter family of rulings, and two base curve endpoints a(0), a(1) is an element of R-3, we consider the problem of constructing a base curve a(t) such that a(t(0)) = a(0), a(t(1)) = a(1), and the resulting surface f(s, t) = a(t) + sb(t) is developable. We formulate the base curve design problem as an optimal control problem, and derive solutions for objective functions that reflect various practical aspects of developable surface design, e.g,, minimizing the arc length of the base curve, keeping the line of regression distant from tire base curve, and approximating a given arbitrary ruled surface by a developable surface. By drawing upon the large body available results for the optimal control of linear systems with quadratic criteria, our approach provides a flexible method for designing developable surfaces that are optimized for various criteria.},
   year = {2002}
}

