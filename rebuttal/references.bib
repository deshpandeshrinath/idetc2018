}

@article{Kamnik2003,
   author = {Kamnik, R. and Bajd, T.},
   title = {Robot assistive device for augmenting standing-up capabilities in impaired people},
   journal = {Iros 2003: Proceedings of the 2003 Ieee/Rsj International Conference on Intelligent Robots and Systems, Vols 1-4},
   pages = {3606-3611},
