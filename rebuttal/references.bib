@article{
   author = {Ozatevli, S. and Ozden, A. and Itil, O. and Akkoclu, A.},
   title = {Comparison of the Sit-to-Stand Test with 6 min walk test in patients with chronic obstructive pulmonary disease},
   journal = {Respiratory Medicine},
   volume = {101},
